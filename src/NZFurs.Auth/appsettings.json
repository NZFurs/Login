{
  "Argon2i": {
    "DegreeOfParallelism": 2,
    "Iterations": 40,
    "MemorySize": 8192,
    "HashSize": 128,
    "SaltSize": 64
  },
  "Azure": {
    "KeyVault": {
      "KeyVault": "nzfurs-auth-jwt",
      "KeyName": "Production"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=Data/Database/sqlite.db;"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    }
  },


  "StsConfig": {
    "StsServerIdentityUrl": "__StsServerIdentityUrl__",
    "AngularClientUrl": "__AngularClientUrl__",
    "AngularClientIdTokenOnlyUrl": "__AngularClientIdTokenOnlyUrl__"
  },
  "UseLocalCertStore": "__UseLocalCertStore__",
  "CertificateThumbprint": "__CertificateThumbprint__",
  "EmailSettings": {
    "SendGridApiKey": "__SendGridApiKey__",
    "SenderEmailAddress": "__SenderEmailAddress__"
  }
}