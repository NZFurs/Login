{
  "Azure": {
    "KeyVault": {
      "KeyVault": "nzfurs-auth-jwt-dev",
      "KeyName": "Development"
    }
  },
  "IdentityServer4": {
    "PublicOrigin": "http://localhost:5000"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Information",
        "Microsoft": "Information"
      }
    }
  },


  "StsConfig": {
    "StsServerIdentityUrl": "https://localhost:44318",
    "ClientUrl": "https://localhost:44311"
  },
  "UseLocalCertStore": "true",
  "CertificateThumbprint": "TODO_GET_FROM_DEPLOYMENT_SERVER_CERT",
  "EmailSettings": {
    "SendGridApiKey": "TODO_EMAIL_API_KEY",
    "SenderEmailAddress": "TODO_EMAIL_ADDRESS"
  }
}