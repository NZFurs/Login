{
  "Azure": {
    "KeyVault": {
      "KeyVault": "nzfurs-auth-jwt",
      "KeyName": "Production"
    }
  },
  "IdentityServer4": {
    "PublicOrigin": "https://login.furry.nz"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    }
  },


  "StsConfig": {
    "StsServerIdentityUrl": "https://localhost:44318",
    "ClientUrl": "https://localhost:44311"
  },
  "UseLocalCertStore": "true",
  "CertificateThumbprint": "TODO_GET_FROM_DEPLOYMENT_SERVER_CERT",
  "EmailSettings": {
    "SendGridApiKey": "TODO_EMAIL_API_KEY",
    "SenderEmailAddress": "TODO_EMAIL_ADDRESS"
  }
}