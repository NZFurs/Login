{
  "Azure": {
    "KeyVault": {
      "KeyVault": "nzfurs-auth-jwt",
      "KeyName": "Production"
    }
  },
  "ConnectionStrings": {
    "AzureServiceTokenProvider": "RunAs=App;AppId={AppId};TenantId={TenantId};AppKey={ClientSecret}"
  },
  "Data": {
    "Database": {
      "Provider": "postgres"
    }
  },
  "HttpServer": {
    "Endpoints": {
      "Http": {
        "Url": "http://login.furry.nz:80"
      },
      "Https": {
        "Url": "https://login.furry.nz:443"
      }
    }
  }
}