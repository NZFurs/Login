{
  "Azure": {
    "KeyVault": {
      "KeyVault": "nzfurs-auth-jwt",
      "KeyName": "Production"
    }
  },
  "ConnectionStrings": {
    "AzureServiceTokenProvider": "RunAs=App"
  },
  "IdentityServer4": {
    "PublicOrigin": "https://login.furry.nz"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    }
  }
}